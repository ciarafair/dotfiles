{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
		"type": "file",
		"source": "$HOME/Pictures/space_invader1.txt"
	},
	"modules": [
		{
      "type": "version",
      "key": "Fastfetch",
			"format": "v{version}"
    },
